@model ClubWebModel
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Страница клуба - Клуб программистов</title>
    <script src="~/lib/htmx/htmx.js"></script>
</head>
<body>
    <h4>Страница клуба программистов</h4>
    <p>Согласование темы следующего совещания.</p>
    <p>
        Ваше имя: <strong>@Model.Current.Name</strong>
    </p>
    <div id="club" hx-get hx-action="Index"
         hx-route-id="@Model.Id"
         hx-trigger="every 1s">
        <partial name="Partial/ClubPartial" model="Model"/>
    </div>
</body>
</html>

